{
  "main": "src/bootstrapper.js",
  "name": "clean-torrent",
  "description": "A clean torrent client written with electron",
  "repository": {
    "type": "git",
    "url": "https://github.com/scwe/clean.git"
  },
  "scripts": {
    "start": "electron .",
    "standard": "standard --verbose --fix | snazzy",
    "standard-message": "echo 'Pre-commit standard --fix' && exit 0"
  },
  "license": "MIT",
  "version": "0.1.0",
  "dependencies": {
    "babel-preset-es2015": "*",
    "babel-preset-react": "*",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "*",
    "bencode": "^0.10.0",
    "bn.js": "^4.11.6",
    "electron": "^1.6.11",
    "javascript-state-machine": "^2.3.5",
    "parse-torrent": "*",
    "pre-commit": "^1.2.2",
    "randombytes": "^2.0.3",
    "react": "*",
    "react-dom": "*",
    "react-redux": "*",
    "react-router": "3.*",
    "redux": "*",
    "sha1": "^1.1.1",
    "shortid": "^2.2.6",
    "snazzy": "^7.0.0",
    "socket.io": "*",
    "standard": "^10.0.2",
    "torrent-discovery": "^8.1.1",
    "torrent-stream": "*",
    "underscore": "^1.8.3"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ],
    "compact": false
  },
  "pre-commit": [
    "npm run standard",
    "npm run standard-message"
  ]
}
